                                <p>DR View</p>
                            </a>
                        </li>
                      @*  <li>
                            <a href="#">
                                <i class="material-icons">library_books</i>
                                <p>Resources</p>
                            </a>
                        </li>*@
                    </ul>
                </div>
            </div>

            <div class="main-panel" >
                <div style="height: 70px; position: relative; top: 0px; margin-left: auto; margin-right: auto;">
                    <div class="PrjTitl">
                        <a class="navbar-brand" href="#">
                            <h3 class="hidden-xs" style="color: #000; font-weight: bold; letter-spacing: 0.7px; line-height: 1; margin: 0px;">HOME</h3>
                            <h3 class="hidden-xl hidden-lg hidden-md hidden-sm" style="color: #000; font-weight: bold; letter-spacing: 0.7px; line-height: 1; margin: 0px;">PML PD</h3>
                        </a>
                        <a href="@Url.Action("signout", "Home")" class="btn btn-danger pull-right" style="margin-right: 15px; padding: 6px 12px; background-color:#9c27b0 !important">
                            <i class="material-icons" style="font-size: 28px">power_settings_new</i>
                            <br />
                            <small>HOME</small></a>
                      
                    </div>
                </div>

                <div style="min-width: 800px; overflow: auto !important; width: 100%;">
                    <nav class="navbar navbar-transparent navbar-absolute" style="position: relative; margin-bottom: 5px; margin-top: 5px;">
                        <div class="container-fluid">
                            <div class="navbar-header" style="border-bottom: 2px solid #a1aff3; border-top: 5px double #a1aff3; text-align: right;">
                                <div class="hidden-lg hidden-md" style="text-align: right; display: inline-block; margin-top: 8px;">Menu</div>
                                <button type="button" class="navbar-toggle" data-toggle="collapse" style="margin-top: 0px; margin-bottom: 0px;">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            </div>
                        </div>
                    </nav>
                    <div class="content" style="margin-left: 15px;">
                        @RenderBody()
                    </div>
                    <footer class="footer">
                        <div class="container-fluid">
                            <p class="copyright text-center">
                                &copy;
                       
                    <script>
                        document.write(new Date().getFullYear())
                    </script>
                                Test Copy.
                   
                            </p>
                        </div>
                    </footer>
                </div>
            </div>
        </div>
        }
        catch (Exception ex) {  }
   
</body>
