<body>
 
   
        @try
        {
            
            
        <div class="wrapper">
            <div class="sidebar" data-color="purple" data-image="~/Content/img/cover.jpeg" style="background-image: url('../Content/img/cover.jpeg')">
                <div class="logo">
                    <a href="@Url.Action("index", "Home")" class="simple-text">
                      
                    </a>
                </div>
                <div class="sidebar-wrapper">
                    <ul class="nav ">
                        <li >
                            <a href="@Url.Action("signout", "Home")">
